{
  "name": "ferrysutanto19/laravel-ews",
  "description": "PHP EWS wrapper for laravel",
  "keywords": ["php", "ews", "php-ews", "laravel", "laravel-ews", "laravel ews"],
  "type": "library",
  "homepage": "https://github.com/ferrysutanto19/laravel-ews",
  "license": "MIT",
  "authors": [
    {
      "name": "Ferry Sutanto",
      "email": "ferry.sutanto.19@gmail.com",
      "homepage": "https://github.com/ferrysutanto19"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "php-ews/php-ews": "dev-master",
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.0",
  },
  "autoload": {
    "psr-4": {
      "Telegram\\Bot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Telegram\\Bot\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
